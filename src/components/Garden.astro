---
import "../css/garden.css";
import Tree from "../assets/tree-black00.svg";
import Plant from "../assets/plant-black00.svg";
const { backgroundId } = Astro.props;
---

<div id={backgroundId} class="garden-background">
    <div class="garden">
        <Tree class="tree" />
        <Plant class="plant mid-plant" />
        <Plant class="plant right-plant" />
    </div>
</div>
