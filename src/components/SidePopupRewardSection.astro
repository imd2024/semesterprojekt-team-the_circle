---
import "../css/SidePopUpStyle.css";
const { title, icons } = Astro.props;
---

<section class="popup-section">
    <h3>{title}</h3>
    <div class="icon-container">
    {icons.map((icon) => {
      const IconComponent = icon.src
      return <IconComponent class="reward-icon" alt={icon.alt ?? ''} width="48" height="48"/>
    })}
  </div>
</section>
